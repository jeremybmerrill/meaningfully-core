{
  "name": "@meaningfully/core",
  "version": "0.1.3",
  "description": "Semantic search over CSVs",
  "main": "./dist/meaningfully-core.cjs.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/meaningfully-core.es.js",
      "require": "./dist/meaningfully-core.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "jeremybmerrill.com",
  "homepage": "https://github.com/jeremybmerrill/meaningfully",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.json --composite false",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "test": "vitest --silent=false --disable-console-intercept",
    "build": "vite build"
  },
  "dependencies": {
    "@llamaindex/azure": "^0.1.31",
    "@llamaindex/google": "^0.3.18",
    "@llamaindex/mistral": "^0.1.14",
    "@llamaindex/ollama": "^0.1.14",
    "@llamaindex/openai": "^0.4.14",
    "@llamaindex/postgres": "^0.0.64",
    "@llamaindex/weaviate": "^0.0.36",
    "better-sqlite3": "^11.8.1",
    "js-tiktoken": "^1.0.8",
    "llamaindex": "^0.11.14",
    "natural": "^8.1.0",
    "openai": "^5.9.0",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.14.8",
    "@types/papaparse": "^5.3.16",
    "bufferutil": "^4.0.9",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "tslib": "^2.6.3",
    "typescript": "^5.7.3",
    "utf-8-validate": "^6.0.5",
    "ws": "^8.18.3"
  }
}
